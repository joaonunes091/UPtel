@model IEnumerable<UPtel.Models.Meses>

@{
    ViewData["Title"] = "Index";
}

<section id="secondhero">
    <div class="container">
        <div class="row justify-content-center ">
            <div class="col-xl-8 text-white">
                <h2 class="font">Meses</h2>

               
                <table class="table">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Mes)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Mes)
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>